   "source": [
    "def isbalanced(s):\n",
    "    c = 0\n",
    "    for i in s:\n",
    "        if i=='{':\n",
    "            c = c^1\n",
    "        if i == '}':\n",
    "            c = c^1\n",
    "    if(c==0):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "s=input(\"Enter a string of brackets:\")\n",
    "print(\"Given string is balanced:\",isbalanced(s))"
   ]
